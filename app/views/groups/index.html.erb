<h1>Groups/h1>
<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Person</th>
      <th>Building</th>
      <th>Space</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @groups.each do |group| %>
      <tr>
        <td><%= group.name %></td>
        <td><%= group.description %></td>
        <td><%= group.phone_number %></td>
        <td><%= group.email_address %></td>
        <td><%= group.person %></td>
        <td><%= group.building %></td>
        <td><%= group.space %></td>
        <td><%= link_to 'Show', group %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

