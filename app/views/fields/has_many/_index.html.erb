<% unless field.resource.class.to_s == "Service" || field.resource.class.to_s == "Policy" %>
	<ul style="list-style-type: none;padding-left: 0px">
	<% field.data.each do |category| %>
		<li><%= category.label %></li>
	<% end %>
	</ul>
<% else %>
	<%= field.data.take.label unless field.data.take.nil? %>
<% end %>