<% if @webpage.file_uploads.present? %>
  <h2><%= t("manifold.webpages.attachments.files") %></h2>
      <ul class="list-unstyled">
      <% @webpage.file_uploads.each do |fu| %>
        <li>
          <%= image_tag "pdf.png", class: "decorative", alt: "", style: "width:42px" %> 
          <%= link_to fu.name, url_for(fu.file), disposition: "attachment" %>
        </li>
      <% end %>
      </ul>
<% end %>
