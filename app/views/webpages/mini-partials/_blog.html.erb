<h2 class="links-title">
  <a style="cursor:pointer" data-toggle="collapse" data-target="#blog" aria-expanded="true" aria-controls="blog"> 
  	Research Blog
  </a>
</h2>

<ul class="list-unstyled navbar-collapse collapse show" id="blog">
<% @event_links.each do |link| %>
	<li><%= link_to link.label, event_path(link.id) %></li>
<% end %>
	<li class="text-right all-events"><%= link_to "View all", blog_path(@blog), style: "text-decoration:underline" %> ></li>
</ul>
