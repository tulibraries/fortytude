<div class="row" id="seagreen">
  <div class="col-11 offset-1">
    <h1>
      Buildings
    </h1>
  </div>
</div>

<div class="container">
<div class="row">
<% unless @buildings.nil? %>
<div class="col">
	<ul>
    <% @buildings.each do |building| %>
      <li>
        <%= link_to building.name, building %>
      </li>
    <% end %>
  </ul>
</div>
<% end %>
</div>
</div>