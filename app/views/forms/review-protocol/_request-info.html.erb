<%= f.input :protocol_title, as: :text, 
				label: t("manifold.forms.review_protocol.protocol_title"), 
				hint: t("manifold.forms.review_protocol.protocol_title_hint"), 
				required: true %>
<%= f.input :review_update, as: :radio_buttons, 
				label: t("manifold.forms.review_protocol.review_update"), 
				collection: t("manifold.forms.fields.boolean.yes_no").map{ |k,v| v },  
				hint: t("manifold.forms.review_protocol.review_update_hint"), 
				required: true %>
<%= f.input :rationale, as: :text, 
				label: t("manifold.forms.review_protocol.rationale"), 
				required: true %>
<%= f.input :explicit_statement, as: :text, 
				label: t("manifold.forms.review_protocol.explicit_statement"), 
				required: true %>
<%= f.input :information_sources, as: :text, 
				label: t("manifold.forms.review_protocol.information_sources"), 
				hint: t("manifold.forms.review_protocol.information_sources_hint"), 
				required: true %>
<%= f.input :included_keywords, as: :text, 
				label: t("manifold.forms.review_protocol.included_keywords"),
				required: true %>
<%= f.input :excluded_keywords, as: :text, 
				label: t("manifold.forms.review_protocol.excluded_keywords"),
				required: true %>
<%= f.input :data_plan, as: :text, 
				label: t("manifold.forms.review_protocol.data_plan"), 
				required: true %>
<%= f.input :independent_reviewers, as: :radio_buttons, 
				label: t("manifold.forms.review_protocol.independent_reviewers"), 
				collection: t("manifold.forms.fields.boolean.yes_no"), 
				hint: t("manifold.forms.review_protocol.independent_reviewers_hint"),  
				required: true %>
<%= f.input :outcomes, as: :text, 
				label: t("manifold.forms.review_protocol.outcomes"), 
				hint: t("manifold.forms.review_protocol.outcomes_hint"), 
				required: true %>
<%= f.input :quantitative_analysis, as: :radio_buttons, 
				label: t("manifold.forms.review_protocol.quantitative_analysis"), 
				collection: t("manifold.forms.fields.boolean.yes_no")
							.merge(t("manifold.forms.fields.boolean.undecided"))
							.map{ |k,v| v }  %>
<%= f.input :quantitative_analysis_other, label: false, 
				placeholder: t("manifold.forms.fields.other_placeholder"),
				wrapper_html: { class: "specify-other" } %>
<%= f.input :bio_statistician, as: :radio_buttons, 
				label: t("manifold.forms.review_protocol.bio_statistician"),
				collection: t("manifold.forms.fields.boolean.yes_no")
							.merge(t("manifold.forms.fields.boolean.undecided"))
							.map{ |k,v| v } %>
<%= f.input :bio_statistician_other, 
				label: false, 
				placeholder: t("manifold.forms.fields.other_placeholder"),
				wrapper_html: { class: "specify-other" } %>
<%= f.input :summary_description, as: :text, 
				label: t("manifold.forms.review_protocol.sumary_description") %>
<%= f.input :evidence_assessment, as: :text, 
				label: t("manifold.forms.review_protocol.evidence_assessment"), 
				hint: t("manifold.forms.review_protocol.evidence_assessment_hint"), 
				required: true %>
<%= f.input :citations, as: :text, 
				label: t("manifold.forms.review_protocol.citations"), 
				required: true %>
<%= f.input :librarian_contact, as: :radio_buttons, 
				label: t("manifold.forms.review_protocol.librarian_contact"), 
				collection: t("manifold.forms.fields.boolean.yes_no")
							.merge(t("manifold.forms.fields.boolean.undecided"))
							.map{ |k,v| v }, 
				required: true %>
<%= f.input :librarian_contact_other, 
				label: false, 
				placeholder: t("manifold.forms.fields.other_placeholder"),
				wrapper_html: { class: "specify-other" } %>
<%= f.input :authorship_permission, as: :radio_buttons, 
				label: t("manifold.forms.review_protocol.authorship_permission"), 
				collection: t("manifold.forms.fields.boolean.yes_no")
							.merge(t("manifold.forms.fields.boolean.undecided"))
							.map{ |k,v| v },
				required: true %>
<%= f.input :authorship_permission_other, label: false, 
				placeholder: t("manifold.forms.fields.other_placeholder"),
				wrapper_html: { class: "specify-other" } %>
<%= f.input :other_reviews, as: :radio_buttons, 
				label: t("manifold.forms.review_protocol.other_reviews"),
				collection: t("manifold.forms.fields.boolean.yes_no")
							.merge(t("manifold.forms.fields.boolean.undecided"))
							.map{ |k,v| v },
				hint: t("manifold.forms.fields.other_reviews_hint"),
				required: true %>
<%= f.input :other_reviews_other, 
				label: false, 
				placeholder: t("manifold.forms.fields.other_placeholder"),
				wrapper_html: { class: "specify-other" } %>
<%= f.input :publication_journal, as: :text, 
				label: t("manifold.forms.review_protocol.publication_journal"), 
				hint: t("manifold.forms.review_protocol.publication_journal_hint") %>
