{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "The Data Schema",
      "required": [
        "id",
        "type",
        "attributes",
        "relationships",
        "links"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/data/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "1"
          ],
          "pattern": "^(.*)$"
        },
        "type": {
          "$id": "#/properties/data/properties/type",
          "type": "string",
          "title": "The Type Schema",
          "default": "",
          "examples": [
            "building"
          ],
          "pattern": "^(.*)$"
        },
        "attributes": {
          "$id": "#/properties/data/properties/attributes",
          "type": "object",
          "title": "The Attributes Schema",
          "required": [
            "name",
            "address1",
            "address2",
            "temple_building_code",
            "coordinates",
            "google_id",
            "campus",
            "phone_number",
            "description"
          ],
          "properties": {
            "name": {
              "$id": "#/properties/data/properties/attributes/properties/name",
              "type": "string",
              "title": "The Name Schema",
              "default": "",
              "examples": [
                "Charles Library"
              ],
              "pattern": "^(.*)$"
            },
            "address1": {
              "$id": "#/properties/data/properties/attributes/properties/address1",
              "type": "string",
              "title": "The Address1 Schema",
              "default": "",
              "examples": [
                "1210 Pollett Walk"
              ],
              "pattern": "^(.*)$"
            },
            "addresss2": {
              "$id": "#/properties/data/properties/attributes/properties/address2",
              "type": "string",
              "title": "The Address2 Schema",
              "default": "",
              "examples": [
                "Room 110"
              ],
              "pattern": "^(.*)$"
            },
            "description": {
              "$id": "#/properties/data/properties/attributes/properties/description",
              "type": "string",
              "title": "The Description Schema",
              "default": "",
              "examples": [
                "Temple University Main Library"
              ],
              "pattern": "^(.*)$"
            },
            "coordinates": {
              "$id": "#/properties/data/properties/attributes/properties/coordinates",
              "type": "string",
              "title": "The Coordinates Schema",
              "default": "",
              "examples": [
                ".9809877,-75.1540588"
              ],
              "pattern": "^(.*)$"
            },
            "phone_number": {
              "$id": "#/properties/data/properties/attributes/properties/phone_number",
              "type": "string",
              "title": "The Phone_number Schema",
              "default": "",
              "examples": [
                "9580329973"
              ],
              "pattern": "^(.*)$"
            },
            "campus": {
              "$id": "#/properties/data/properties/attributes/properties/campus",
              "type": "string",
              "title": "The Campus Schema",
              "default": "",
              "examples": [
                "Main Campus"
              ]
            },
            "email_address": {
              "$id": "#/properties/data/properties/attributes/properties/email_address",
              "type": "string",
              "title": "The Email_address Schema",
              "default": "",
              "examples": [
                "stanford_koelpin@temple.edu"
              ],
              "pattern": "^(.*)$"
            },
            "photo": {
              "$id": "#/properties/data/properties/attributes/properties/photo",
              "type": "string",
              "title": "The Photo Schema",
              "default": "",
              "examples": [
                "http://test.host/rails/active_storage/blobs/eyJfcmFpbHMiOnsibWVzc2FnZSI6IkJBaHBFUT09IiwiZXhwIjpudWxsLCJwdXIiOiJibG9iX2lkIn19--5bae44b342f201d4510c9c5878fdcb6747e6497d/hal.png"
              ],
              "pattern": "^(.*)$"
            },
            "google_id": {
              "$id": "#/properties/data/properties/attributes/properties/google_id",
              "type": "string",
              "title": "The Google_id Schema",
              "default": "",
              "examples": [
                "AAAA-1111-BBBB-2222-CCCC"
              ],
              "pattern": "^(.*)$"
            }
          }
        },
        "relationships": {
          "$id": "#/properties/data/properties/relationships",
          "type": "object",
          "title": "The Relationships Schema",
          "required": [
            "spaces"
          ],
          "properties": {
            "spaces": {
              "$id": "#/properties/data/properties/relationships/properties/spaces",
              "type": "object",
              "title": "The Spaces Schema",
              "required": [
                "data"
              ],
              "properties": {
                "data": {
                  "$id": "#/properties/data/properties/relationships/properties/spaces/properties/data",
                  "type": "array",
                  "title": "The Data Schema",
                  "items": {
                    "$id": "#/properties/data/properties/relationships/properties/spaces/properties/data/items",
                    "type": "object",
                    "title": "The Items Schema",
                    "required": [
                      "id",
                      "type"
                    ],
                    "properties": {
                      "id": {
                        "$id": "#/properties/data/properties/relationships/properties/spaces/properties/data/items/properties/id",
                        "type": "string",
                        "title": "The Id Schema",
                        "default": "",
                        "examples": [
                          "31"
                        ],
                        "pattern": "^(.*)$"
                      },
                      "type": {
                        "$id": "#/properties/data/properties/relationships/properties/spaces/properties/data/items/properties/type",
                        "type": "string",
                        "title": "The Type Schema",
                        "default": "",
                        "examples": [
                          "space"
                        ],
                        "pattern": "^(.*)$"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "links": {
          "$id": "#/properties/data/properties/links",
          "type": "object",
          "title": "The Links Schema",
          "required": [
            "self"
          ],
          "properties": {
            "self": {
              "$id": "#/properties/data/properties/links/properties/self",
              "type": "string",
              "title": "The Self Schema",
              "default": "",
              "examples": [
                "http://test.host/building/1"
              ],
              "pattern": "^(.*)$"
            }
          }
        }
      }
    }
  }
}
