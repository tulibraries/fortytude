{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "type": "object",
      "title": "Data Wrapper Hash",
      "required": [
        "id",
        "name",
        "description",
        "address1",
        "address2",
        "coordinates",
        "temple_building_code",
        "hours",
        "phone_number",
        "campus",
        "email",
        "google_id",
        "url",
        "created_at",
        "updated_at"
      ],
      "properties": {
        "id": {
          "$id": "#/data/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "0",
          "examples": [
            "3"
          ],
          "pattern": "^([0-9]+)$"
        },
        "type": {
          "$id": "#/data/type",
          "type": "string",
          "title": "Type of Object",
          "enum": [
            "event",
            "person",
            "building"
          ]
        },
        "name": {
          "$id": "#/data/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "Simmy and Harry Ginsburg Health Sciences Library"
          ],
          "pattern": "^(.*)$"
        },
        "description": {
          "$id": "#/data/description",
          "type": "string",
          "title": "The Description Schema",
          "default": "",
          "examples": [
            "<p>The Health Sciences Libraries of Temple University serve the Schools of Medicine, Dentistry, Pharmacy, Podiatry and the College of Public Health.</p>\r\n<p>Opened in June 2009 in the Medical Education and Research Building, the new Ginsburg Health Sciences Library offers group study rooms, wireless access, seating for nearly 1,000, a laptop lending program and more.</p>"
          ]
        },
        "address1": {
          "$id": "#/data/address1",
          "type": "string",
          "title": "The Address1 Schema",
          "default": "",
          "examples": [
            "Medical Education and Research Building, 3500 N. Broad Street"
          ],
          "pattern": "^(.*)$"
        },
        "address2": {
          "$id": "#/data/address2",
          "type": "string",
          "title": "The Address2 Schema",
          "default": "",
          "examples": [
            "Philadelphia, PA 19140"
          ],
          "pattern": "^(.*)$"
        },
        "coordinates": {
          "$id": "#/data/coordinates",
          "type": "string",
          "title": "The Coordinates Schema",
          "default": "",
          "examples": [
            "40.0068696,-75.1522336"
          ],
          "pattern": "^(.*)$"
        },
        "temple_building_code": {
          "$id": "#/data/temple_building_code",
          "type": "string",
          "title": "The Temple_building_code Schema",
          "default": "",
          "examples": [
            "MERB"
          ],
          "pattern": "^(.*)$"
        },
        "hours": {
          "$id": "#/data/hours",
          "type": "string",
          "title": "The Hours Schema",
          "default": "",
          "examples": [
            "ginsburg"
          ],
          "pattern": "^(.*)$"
        },
        "phone_number": {
          "$id": "#/data/phone_number",
          "type": "string",
          "title": "The Phone_number Schema",
          "default": "",
          "examples": [
            "2157072665"
          ],
          "pattern": "^(.*)$"
        },
        "campus": {
          "$id": "#/data/campus",
          "type": "string",
          "title": "The Campus Schema",
          "default": "",
          "examples": [
            "Health Sciences"
          ],
          "pattern": "^(.*)$"
        },
        "email": {
          "$id": "#/data/email",
          "type": "string",
          "title": "The Email Schema",
          "default": "",
          "examples": [
            "hscref@temple.edu"
          ],
          "pattern": "^(.*)$"
        },
        "google_id": {
          "$id": "#/data/google_id",
          "type": "string",
          "title": "The Google_id Schema",
          "default": "",
          "examples": [
            "ChIJcUgQT_i3xokRLELTv3EhIfk"
          ],
          "pattern": "^(.*)$"
        },
        "created_at": {
          "$id": "#/data/created_at",
          "type": "string",
          "title": "The Created_at Schema",
          "default": "",
          "examples": [
            "2018-08-24T13:28:19.846Z"
          ],
          "pattern": "^(.*)$"
        },
        "updated_at": {
          "$id": "#/data/updated_at",
          "type": "string",
          "title": "The Updated_at Schema",
          "default": "",
          "examples": [
            "2019-01-09T20:45:29.229Z"
          ],
          "pattern": "^(.*)$"
        },
        "url": {
          "$id": "#/data/url",
          "type": "string",
          "title": "The Url Schema",
          "default": "",
          "examples": [
            "http://localhost:3000/libraries/3.json"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}
