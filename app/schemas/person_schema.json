{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "The Data Schema",
      "required": [
        "id",
        "type",
        "attributes",
        "relationships",
        "links"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/data/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "1"
          ],
          "pattern": "^(.*)$"
        },
        "type": {
          "$id": "#/properties/data/properties/type",
          "type": "string",
          "title": "The Type Schema",
          "default": "",
          "examples": [
            "person"
          ],
          "pattern": "^(.*)$"
        },
        "attributes": {
          "$id": "#/properties/data/properties/attributes",
          "type": "object",
          "title": "The Attributes Schema",
          "required": [
            "name",
            "first_name",
            "last_name",
            "job_title",
            "email_address",
            "phone_number"
          ],
          "properties": {
            "name": {
              "$id": "#/properties/data/properties/attributes/properties/name",
              "type": "string",
              "title": "The Name Schema",
              "default": "",
              "examples": [
                "Mirtha Larson"
              ],
              "pattern": "^(.*)$"
            },
            "first_name": {
              "$id": "#/properties/data/properties/attributes/properties/first_name",
              "type": "string",
              "title": "The First_name Schema",
              "default": "",
              "examples": [
                "Mirtha"
              ],
              "pattern": "^(.*)$"
            },
            "last_name": {
              "$id": "#/properties/data/properties/attributes/properties/last_name",
              "type": "string",
              "title": "The Last_name Schema",
              "default": "",
              "examples": [
                "Larson"
              ],
              "pattern": "^(.*)$"
            },
            "job_title": {
              "$id": "#/properties/data/properties/attributes/properties/job_title",
              "type": "string",
              "title": "The Job_title Schema",
              "default": "",
              "examples": [
                "Dynamic Advertising Orchestrator"
              ],
              "pattern": "^(.*)$"
            },
            "email_address": {
              "$id": "#/properties/data/properties/attributes/properties/email_address",
              "type": "string",
              "title": "The Email_address Schema",
              "default": "",
              "examples": [
                "stanford_koelpin@temple.edu"
              ],
              "pattern": "^(.*)$"
            },
            "phone_number": {
              "$id": "#/properties/data/properties/attributes/properties/phone_number",
              "type": "string",
              "title": "The Phone_number Schema",
              "default": "",
              "examples": [
                "9580329973"
              ],
              "pattern": "^(.*)$"
            },
            "specialties": {
              "$id": "#/properties/data/properties/attributes/properties/specialties",
              "type": "null",
              "title": "The Specialties Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "photo": {
              "$id": "#/properties/data/properties/attributes/properties/photo",
              "type": "string",
              "title": "The Photo Schema",
              "default": "",
              "examples": [
                "http://test.host/rails/active_storage/blobs/eyJfcmFpbHMiOnsibWVzc2FnZSI6IkJBaHBFUT09IiwiZXhwIjpudWxsLCJwdXIiOiJibG9iX2lkIn19--5bae44b342f201d4510c9c5878fdcb6747e6497d/hal.png"
              ],
              "pattern": "^(.*)$"
            },
            "thumbnail_photo": {
              "$id": "#/properties/data/properties/attributes/properties/thumbnail_photo",
              "type": "string",
              "title": "The Thumbnail_photo Schema",
              "default": "",
              "examples": [
                "http://test.host/rails/active_storage/representations/eyJfcmFpbHMiOnsibWVzc2FnZSI6IkJBaHBFUT09IiwiZXhwIjpudWxsLCJwdXIiOiJibG9iX2lkIn19--5bae44b342f201d4510c9c5878fdcb6747e6497d/eyJfcmFpbHMiOnsibWVzc2FnZSI6IkJBaDdCam9MY21WemFYcGxTU0lNTVRBd2VERXdNQVk2QmtWVSIsImV4cCI6bnVsbCwicHVyIjoidmFyaWF0aW9uIn19--1780aa09167cc24cd4beae09f819f90d80b64c3a/hal.png"
              ],
              "pattern": "^(.*)$"
            }
          }
        },
        "relationships": {
          "$id": "#/properties/data/properties/relationships",
          "type": "object",
          "title": "The Relationships Schema",
          "required": [
            "groups",
            "spaces"
          ],
          "properties": {
            "groups": {
              "$id": "#/properties/data/properties/relationships/properties/groups",
              "type": "object",
              "title": "The Groups Schema",
              "required": [
                "data"
              ],
              "properties": {
                "data": {
                  "$id": "#/properties/data/properties/relationships/properties/groups/properties/data",
                  "type": "array",
                  "title": "The Data Schema",
                  "items": {
                    "$id": "#/properties/data/properties/relationships/properties/groups/properties/data/items",
                    "type": "object",
                    "title": "The Items Schema",
                    "required": [
                      "id",
                      "type"
                    ],
                    "properties": {
                      "id": {
                        "$id": "#/properties/data/properties/relationships/properties/groups/properties/data/items/properties/id",
                        "type": "string",
                        "title": "The Id Schema",
                        "default": "",
                        "examples": [
                          "1"
                        ],
                        "pattern": "^(.*)$"
                      },
                      "type": {
                        "$id": "#/properties/data/properties/relationships/properties/groups/properties/data/items/properties/type",
                        "type": "string",
                        "title": "The Type Schema",
                        "default": "",
                        "examples": [
                          "group"
                        ],
                        "pattern": "^(.*)$"
                      }
                    }
                  }
                }
              }
            },
            "spaces": {
              "$id": "#/properties/data/properties/relationships/properties/spaces",
              "type": "object",
              "title": "The Spaces Schema",
              "required": [
                "data"
              ],
              "properties": {
                "data": {
                  "$id": "#/properties/data/properties/relationships/properties/spaces/properties/data",
                  "type": "array",
                  "title": "The Data Schema",
                  "items": {
                    "$id": "#/properties/data/properties/relationships/properties/spaces/properties/data/items",
                    "type": "object",
                    "title": "The Items Schema",
                    "required": [
                      "id",
                      "type"
                    ],
                    "properties": {
                      "id": {
                        "$id": "#/properties/data/properties/relationships/properties/spaces/properties/data/items/properties/id",
                        "type": "string",
                        "title": "The Id Schema",
                        "default": "",
                        "examples": [
                          "31"
                        ],
                        "pattern": "^(.*)$"
                      },
                      "type": {
                        "$id": "#/properties/data/properties/relationships/properties/spaces/properties/data/items/properties/type",
                        "type": "string",
                        "title": "The Type Schema",
                        "default": "",
                        "examples": [
                          "space"
                        ],
                        "pattern": "^(.*)$"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "links": {
          "$id": "#/properties/data/properties/links",
          "type": "object",
          "title": "The Links Schema",
          "required": [
            "self"
          ],
          "properties": {
            "self": {
              "$id": "#/properties/data/properties/links/properties/self",
              "type": "string",
              "title": "The Self Schema",
              "default": "",
              "examples": [
                "http://test.host/people/1"
              ],
              "pattern": "^(.*)$"
            }
          }
        }
      }
    }
  }
}
