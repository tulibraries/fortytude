      ],
      "pattern": "^(.*)$"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2018-12-05T14:24:00.754Z"
