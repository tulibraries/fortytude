{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The Title Schema",
      "default": "",
      "examples": [
        "2nd Annual Winter Concert"
      ],
      "pattern": "^(.*)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "<p>The Lewis Katz School of Medicine's student a cappella singing group, TachyChordia, will be performing a selection of holiday favorites and will lead us in a sing-a-long. Hot cocoa, marshmallows, cookies, candy canes, and chocolate will be served for those who stay for the performance!</p>"
      ],
      "pattern": "^(.*)$"
    },
    "start_time": {
      "$id": "#/properties/start_time",
      "type": "string",
      "title": "The Start_time Schema",
      "default": "",
      "examples": [
        "2018-12-05T12:15:00.000Z"
      ],
      "pattern": "^(.*)$"
    },
    "end_time": {
      "$id": "#/properties/end_time",
      "type": "string",
      "title": "The End_time Schema",
      "default": "",
      "examples": [
        "2018-12-05T12:15:00.000Z"
      ],
      "pattern": "^(.*)$"
    },
    "building_id": {
      "$id": "#/properties/building_id",
      "type": "integer",
      "title": "The Building_id Schema",
      "default": 0,
      "examples": [
        3
      ]
    },
    "space_id": {
      "$id": "#/properties/space_id",
      "type": "integer",
      "title": "The Space_id Schema",
      "default": 0,
      "examples": [
        6
      ]
    },
    "building": {
      "$id": "#/properties/building",
      "type": "string",
      "title": "The Building Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "space": {
      "$id": "#/properties/space",
      "type": "string",
      "title": "The Space Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "address": {
      "$id": "#/properties/address",
      "type": "string",
      "title": "The Address Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "city": {
      "$id": "#/properties/city",
      "type": "string",
      "title": "The City Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "state": {
      "$id": "#/properties/state",
      "type": "string",
      "title": "The State Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "zip": {
      "$id": "#/properties/zip",
      "type": "string",
      "title": "The Zip Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "contact_name": {
      "$id": "#/properties/contact_name",
      "type": "string",
      "title": "The Contact_name Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "contact_email": {
      "$id": "#/properties/contact_email",
      "type": "string",
      "title": "The Contact_email Schema",
