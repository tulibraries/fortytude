        ""
      ],
      "pattern": "^(.*)$"
    },
    "cancelled": {
      "$id": "#/properties/cancelled",
      "type": "boolean",
      "title": "The Cancelled Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "registration_status": {
      "$id": "#/properties/registration_status",
      "type": "boolean",
      "title": "The Registration_status Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "registration_link": {
      "$id": "#/properties/registration_link",
      "type": "string",
      "title": "The Registration_link Schema",
      "default": "",
