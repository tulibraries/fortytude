{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/root.json",
	"type": "object",
	"title": "The Root Schema",
	"required": [
		"data"
	],
	"properties": {
		"data": {
			"$id": "#/properties/data",
			"type": "object",
			"title": "The Data Schema",
			"required": [
				"id",
				"type",
				"attributes"
			],
			"properties": {
				"id": {
					"$id": "#/properties/data/properties/id",
					"type": "string",
					"title": "The Id Schema",
					"default": "0",
					"examples": [
						"2"
					],
					"pattern": "^([0-9]*)$"
				},
				"type": {
					"$id": "#/properties/data/properties/type",
					"type": "string",
					"title": "The Type Schema",
					"default": "group",
					"examples": [
						"building",
						"event",
						"person"
					],
					"pattern": "^(.*)$"
				},
				"attributes": {
					"$id": "#/properties/data/properties/attributes",
					"type": "object",
					"title": "The Attributes Schema",
					"required": [
						"name",
						"label",
						"description",
						"group_type",
						"external",
						"add_to_footer"
					],
					"properties": {
						"name": {
							"$id": "#/properties/data/properties/attributes/properties/name",
							"type": "string",
							"title": "The Name Schema",
							"default": "",
							"examples": [
								"Council of Committees Group"
							],
							"pattern": "^(.*)$"
						},
						"label": {
							"$id": "#/properties/data/properties/attributes/properties/label",
							"type": "string",
							"title": "The Label Schema",
							"default": "",
							"examples": [
								"Council of Committees Group"
							],
							"pattern": "^(.*)$"
						},
						"description": {
							"$id": "#/properties/data/properties/attributes/properties/description",
							"type": "string",
							"title": "The Description Schema",
							"default": "",
							"examples": [
								"The Temple University Ambler Library supports undergraduate and graduate courses, including landscape architecture, horticulture, business, education, criminal justice, and community and regional planning"
							],
							"pattern": "^(.*)"
						}
					}
				}
			}
		}
	}
}
